{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Sid": "AllowListBucketOnlyInternPrefix",
        "Effect": "Allow",
        "Action": [
          "s3:ListBucket"
        ],
        "Resource": "arn:aws:s3:::hashmitech-dev-bucket",
        "Condition": {
          "StringLike": {
            "s3:prefix": [
              "intern-uploads/*"
            ]
          }
        }
      },
      {
        "Sid": "AllowReadWriteOnlyInternFolder",
        "Effect": "Allow",
        "Action": [
          "s3:GetObject",
          "s3:PutObject"
        ],
        "Resource": "arn:aws:s3:::hashmitech-dev-bucket/intern-uploads/*"
      },
      {
        "Sid": "ExplicitlyDenyDeleteEverywhere",
        "Effect": "Deny",
        "Action": [
          "s3:DeleteObject",
          "s3:DeleteObjectVersion"
        ],
        "Resource": "arn:aws:s3:::hashmitech-dev-bucket/*"
      }
    ]
  }
  