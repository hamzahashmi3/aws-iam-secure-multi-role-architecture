{
  "Version": "2012-10-17",
  "Statement": [
      {
          "Sid": "AllowListBucketsForConsole",
          "Effect": "Allow",
          "Action": "s3:ListAllMyBuckets",
          "Resource": "*"
      },
      {
          "Sid": "AllowListSpecificBucket",
          "Effect": "Allow",
          "Action": "s3:ListBucket",
          "Resource": "arn:aws:s3:::hashmi-bucket-dev"
      },
      {
          "Sid": "AllowGetBucketLocation",
          "Effect": "Allow",
          "Action": "s3:GetBucketLocation",
          "Resource": "arn:aws:s3:::hashmi-bucket-dev"
      },
      {
          "Sid": "AllowReadWriteOnlyInternFolder",
          "Effect": "Allow",
          "Action": [
              "s3:GetObject",
              "s3:PutObject"
          ],
          "Resource": "arn:aws:s3:::hashmi-bucket-dev/intern-uploads/*"
      },
      {
          "Sid": "ExplicitlyDenyDelete",
          "Effect": "Deny",
          "Action": [
              "s3:DeleteObject",
              "s3:DeleteObjectVersion"
          ],
          "Resource": "arn:aws:s3:::hashmi-bucket-dev/*"
      }
  ]
}